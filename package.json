{
  "name": "gunapp",
  "version": "0.1.0",
  "description": "Just a minimal community platform.",
  "author": "GunHub",
  "keywords": [
    "gunapp",
    "community",
    "cms"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gunhub/gunapp.git"
  },
  "bugs": "https://github.com/gunhub/gunapp/issues",
  "contributors": "https://github.com/gunhub/gunapp/graphs/contributors",
  "license": "MIT",
  "engines": {
    "node": ">=4.0.0",
    "npm": ">=3.0.0"
  },
  "main": "gunapp.js",
  "scripts": {
    "lint": "eslint .",
    "start:db": "mongod --dbpath data/db",
    "start": "NODE_PATH=source babel-node source/gunapp.js",
    "test:units": "NODE_PATH=source babel-node source/test/unit/index.js",
    "test:routes": "NODE_PATH=source babel-node source/test/functional/routes/index.js",
    "watch": "watch 'clear && npm run -s lint && npm run -s test:units' source",
    "watch:routes": "watch 'clear && npm run -s lint && npm run -s test:routes' source",
    "pretest": "npm run -s lint",
    "test": "npm run -s test:units && npm run -s test:routes",
    "latest": "updtr"
  },
  "dependencies": {
    "async": "1.5.2",
    "babel-core": "6.6.5",
    "babel-plugin-object-assign": "1.2.1",
    "dotenv": "2.0.0",
    "elemental": "0.5.13",
    "express": "4.13.4",
    "history": "2.0.1",
    "keystone": "ericelliott/keystone",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-router": "2.0.0",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-runtime": "6.6.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-2": "6.5.0",
    "cheerio": "0.20.0",
    "eslint": "1.10.3",
    "eslint-plugin-react": "4.2.0",
    "extend-tape": "1.2.0",
    "supertest": "1.2.0",
    "tape": "4.5.0",
    "tape-jsx-equals": "1.0.0",
    "updtr": "0.1.7",
    "watch": "0.17.1",
    "webpack": "1.12.14",
    "webpack-dev-middleware": "1.5.1",
    "webpack-hot-middleware": "2.9.1"
  }
}
